CREATE TABLE category (
    id INTEGER PRIMARY KEY NOT NULL,
    name TEXT NOT NULL
);

insertCategory:
INSERT OR REPLACE INTO category
VALUES (?,?);

deleteCategory:
DELETE FROM category
WHERE id = ?;

getAllCategories:
SELECT *
FROM category;


